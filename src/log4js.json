{
    "appenders": {
        "commission": {
            "type": "file",
            "filename": "commission.log",
            "layout": {
                "type": "messagePassThrough"
            },
            "flags": "w"
        },
        "contractor": {
            "type": "file",
            "filename": "contractor.log",
            "layout": {
                "type": "messagePassThrough"
            },
            "flags": "w"
        },
        "debugLog": {
            "type": "file",
            "filename": "commission.debug",
            "layout": {
                "type": "messagePassThrough"
            },
            "flags": "w"
        },
        "debug": {
            "type": "stderr"
        },
        "info": {
            "type": "stdout"
        },
        "warn": {
            "type": "stdout"
        },
        "error": {
            "type": "stderr"
        },
        "fatal": {
            "type": "stderr"
        },
        "webEcho": {
            "type": "stderr"
        }
    },
    "categories": {
        "default": {
            "appenders": [
                "commission",
                "info"
            ],
            "level": "info"
        },
        "contractor": {
            "appenders": [
                "contractor",
                "info"
            ],
            "level": "info"
        },
        "debug": {
            "appenders": [
                "debug",
                "debugLog"
            ],
            "level": "debug"
        },
        "info": {
            "appenders": [
                "info",
                "debugLog"
            ],
            "level": "info"
        },
        "warn": {
            "appenders": [
                "warn",
                "debugLog"
            ],
            "level": "warn"
        },
        "error": {
            "appenders": [
                "error",
                "debugLog"
            ],
            "level": "error"
        },
        "fatal": {
            "appenders": [
                "fatal",
                "debugLog"
            ],
            "level": "fatal"
        },
        "webEcho": {
            "appenders": [
                "webEcho"
            ],
            "level": "info"
        }
    }
}